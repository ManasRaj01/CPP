#include <iostream>
using namespace std;

// Inline function to add two integers
inline int add(int a, int b) {
    return a + b;
}

// Inline function to add three integers
inline int add(int a, int b, int c) {
    return a + b + c;
}

// Inline function to add two floating-point numbers
inline float add(float a, float b) {
    return a + b;
}

// Inline function to add four integers
inline int add(int a, int b, int c, int d) {
    return a + b + c + d;
}

int main() {
    // Call different overloaded versions
    cout << "Sum of 5 and 10 (int): " << add(5, 10) << endl;
    cout << "Sum of 3, 7, and 2 (int): " << add(3, 7, 2) << endl;
    cout << "Sum of 4.5 and 3.2 (float): " << add(4.5f, 3.2f) << endl;
    cout << "Sum of 1, 2, 3, and 4 (int): " << add(1, 2, 3, 4) << endl;

    return 0;
}
